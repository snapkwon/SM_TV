<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--state enable-->
    <item android:drawable="@drawable/btn_default_state_pressed" android:state_enabled="true" android:state_pressed="true" />
    <item android:drawable="@drawable/btn_default_state_normal" android:state_enabled="true" android:state_pressed="false" />
    <item android:drawable="@drawable/btn_default_state_pressed" android:state_enabled="true" android:state_focused="true" />
    <!--state disable-->
    <item android:drawable="@drawable/btn_default_state_disable" android:state_enabled="false" />
    <!--default-->
    <item android:drawable="@drawable/btn_default_state" />
</selector>